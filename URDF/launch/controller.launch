<launch>

<rosparam file="$(find not_splited_pih2_description)/launch/controller.yaml" command="load"/>
<node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" ns="not_splited_pih2" args="Rev8_position_controller Rev9_position_controller Rev10_position_controller joint_state_controller "/>
<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
  <remap from="/joint_states" to="/not_splited_pih2/joint_states"/>
</node>

</launch>